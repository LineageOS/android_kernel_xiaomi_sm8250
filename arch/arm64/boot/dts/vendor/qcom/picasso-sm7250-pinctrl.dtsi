/*for G7A SM7250 pinctrl*/

&tlmm {

	cam_sensor_wide_ldo_en: cam_sensor_wide_ldo_en {
		/* RESET REAR2 */
		mux {
			pins = "gpio127";
			function = "gpio";
		};

		config {
			pins = "gpio127";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

